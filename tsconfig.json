{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": true,
    "module": "es6",
    "target": "es5",
    "allowJs": true,
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "*": [
        "./*"
      ],
      "CSFrontend/*": [
        "src/CSFrontend/*"
      ],
      "utils/*": [
        "src/utils/*"
      ]
    }
  },
  "files": ["src/index.ts"]
}
